{
  "extends": ["next/core-web-vitals", "prettier"],
  "root": true,
  "rules": {
    "no-var": "error",
    "no-unused-vars": "error",
    "no-duplicate-imports": "error",
    "prefer-const": "warn",
    "react/jsx-sort-props": "off",
    "max-len": ["error", 120],
    "arrow-body-style": ["error", "always"]
    // "arrow-parens": ["error", "always"]
  }
}
